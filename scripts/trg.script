function upgrade_gun(actor, type, amount)
	if db.actor == nil then return false end
if type == "finish" then
	local texts = { "Улучшение завершено!", "Оружие улучшено!", "Апгрейд установлен!" }
		local task_texture, task_rect = get_texture_info("ui_iconsTotal_weapons")
		local news_text = "%c[255,160,160,160]"..game.translate_string(texts[math.random(3)]).."\\n".."%c[default]"..game.translate_string(amount)
		
		if db.actor:is_talking() then
			db.actor:give_talk_message(news_text, task_texture, task_rect, "iconed_answer_item")
		else
			db.actor:give_game_news(news_text, task_texture, task_rect, 0, 6000)
		end		
	end
if type == "gnhem" then
	local texts = "Нехватает денег"		
