function StoreActorItemsInTable(ItemRank)
  Pre = 0
    db.actor:iterate_inventory(function(dummy, Item)
      for k, v in pairs(all_items) do
        if k >= ItemRank - 0.5 then
            if Item:section() == v then
                Pre = 1
				tActorItems = {}
			    table.insert(tActorItems, v)
            end
          end
       end
     end
   ,nil)
end
