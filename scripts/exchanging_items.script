end

function Get_Number_Item(ItemSec, Table)
    for k, v in pairs(Table) do
        if v == ItemSec then
            return k
        end
    end 
end	

function GetRndItemFromTable(table)
  local t = {}
    for _, v in pairs(table) do
        t[#t+1] = v
    end
    if next(t) then
        return t[math.random(#t)]
    end
end  







