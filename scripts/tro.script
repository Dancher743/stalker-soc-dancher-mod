local gun_condition = 0

function delete_gun(gun_name,upgrade_gun_name)
	local item_weap_unupg = db.actor:object(gun_name)
	if delete_money(item_weap_unupg:cost(),"not","within") == "have_money" then
	upgrade_gun(db.actor, "finish", gun_name)
	delete_guns(gun_name)
	local objecture = alife():create(upgrade_gun_name,db.actor:position(),db.actor:level_vertex_id(),db.actor:game_vertex_id(),db.actor:id())
	delete_money(item_weap_unupg:cost(),"yes","within")
	end
	if delete_money(item_weap_unupg:cost(),"not","within") == "dont_have_money" then
	upgrade_gun(db.actor, "gnhem", gun_name)
	end
end	


function upgrade_gun(actor, type, amount)
if db.actor == nil then 
	return false 
end
if type == "finish" then
	local texts = { "Улучшение завершено!", "Оружие улучшено!", "Апгрейд установлен!" }
		local task_texture, task_rect = get_texture_info("ui_iconsTotal_weapons")
		local news_text = "%c[255,160,160,160]"..game.translate_string(texts[math.random(3)]).."\\n".."%c[default]"..game.translate_string(amount)
		
		if db.actor:is_talking() then
			db.actor:give_talk_message(news_text, task_texture, task_rect, "iconed_answer_item")
		else
			db.actor:give_game_news(news_text, task_texture, task_rect, 0, 6000)
		end		
end
if type == "gnhem" then
		local texts = "Нехватает денег"		
		local task_texture, task_rect = get_texture_info("ui_iconsTotal_lost_money")
		local news_text = "%c[255,160,160,160]"..game.translate_string(texts).."\\n".."%c[default]"..game.translate_string(amount)
		
		if db.actor:is_talking() then
			db.actor:give_talk_message(news_text, task_texture, task_rect, "iconed_answer_item")
		else
			db.actor:give_game_news(news_text, task_texture, task_rect, 0, 6000)
		end	
end
end

